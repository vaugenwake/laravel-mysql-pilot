{
    "name": "vaugenwake/laravel_mysql_pilot",
    "description": "A simple set of commands to backup :package_description restore mysql in your Laravel app.",
    "keywords": [
        "vaugenwake",
        "laravel",
        "laravel_mysql_pilot"
    ],
    "homepage": "https://github.com/vaugenwake/laravel_mysql_pilot",
    "license": "MIT",
    "authors": [
        {
            "name": "Vaugen Wakeling",
            "email": "vaugen.wakeling@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Vaugenwake\\MysqlPilot\\": "src",
            "Vaugenwake\\MysqlPilot\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vaugenwake\\MysqlPilot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "sniff": "./vendor/bin/phpcs",
        "sniff-fix": "./vendor/bin/phpcbf"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vaugenwake\\MysqlPilot\\MysqlPilotServiceProvider"
            ],
            "aliases": {
                "MysqlPilot": "Vaugenwake\\MysqlPilot\\MysqlPilotFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
